{
  "name": "@api3/oev-seeker",
  "version": "0.0.1",
  "keywords": [],
  "license": "MIT",
  "engines": {},
  "files": [
    "dist",
    "src"
  ],
  "main": "./dist/src/npm-exports.js",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/api3dao/oev-seeker"
  },
  "scripts": {
    "build": "pnpm clean && pnpm contracts:compile:force && pnpm run tsc:build",
    "clean": "rimraf ./artifacts/ ./build ./cache ./coverage/ ./dist",
    "contracts:compile:force": "hardhat compile --force",
    "contracts:compile": "hardhat compile",
    "dev:eth-node": "hardhat node",
    "docker:build": "docker run -it --init --env-file ./.env --rm api3/oev-seeker:latest",
    "docker:build:amd64": "docker buildx build --target oev-bot --platform linux/amd64 --tag api3/oev-seeker:latest .",
    "docker:run": "docker run -it --init --env-file ./.env --rm api3/oev-seeker:latest",
    "eslint:check": "eslint --report-unused-disable-directives --cache --ext js,ts . --max-warnings 0",
    "eslint:fix": "pnpm run eslint:check --fix",
    "orbit-bot:cli-utils": "pnpm ts-node src/orbit-bot/cli-utils.ts",
    "orbit-bot:oev-bot": "pnpm ts-node src/orbit-bot/oev-bot.ts",
    "prepare": "husky",
    "prettier:check": "prettier --check \"./**/*.{js,ts,md,json,html}\"",
    "prettier:fix": "prettier --write \"./**/*.{js,ts,md,json,html}\"",
    "release:push": "ts-node scripts/push-ecr-release.ts",
    "release:tag": "ts-node scripts/tag-and-release.ts",
    "test:e2e": "jest --config=jest-e2e.config.js",
    "test": "jest --config=jest-unit.config.js",
    "tsc:build": "tsc -p tsconfig.build.json",
    "tsc": "tsc -p tsconfig.json"
  },
  "dependencies": {
    "@api3/chains": "^7.2.1",
    "@api3/commons": "^0.9.1",
    "@api3/contracts": "4.1.1",
    "@api3/promise-utils": "^0.4.0",
    "@openzeppelin/contracts": "^5.0.2",
    "dotenv": "^16.4.5",
    "immer": "^10.1.1",
    "lodash": "^4.17.21",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-ethers": "^3.0.6",
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@octokit/rest": "^20.1.1",
    "@octokit/types": "^13.5.0",
    "@total-typescript/ts-reset": "^0.5.1",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.5",
    "@types/node": "^20.14.2",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.6.0",
    "ethers": "^6.13.0",
    "hardhat": "^2.22.5",
    "husky": "^9.0.11",
    "is-wsl": "2.2.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.1",
    "rimraf": "^5.0.7",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "typechain": "^8.3.2",
    "typescript": "^5.4.5"
  }
}
